* text=auto

# Unity text assets should be normalized to LF
*.cs text eol=lf
*.meta text eol=lf
*.unity text eol=lf
*.prefab text eol=lf
*.asset text eol=lf
*.shader text eol=lf
*.cginc text eol=lf
*.txt text eol=lf
*.md text eol=lf

# Binary assets
*.png binary
*.jpg binary
*.jpeg binary
*.tga binary
*.gif binary
*.wav binary
*.mp3 binary
*.fbx binary
*.psd binary
